<%- include("partials/header") -%> 

<h1>Compose</h1>
<form action="/compose" method="post">
    <label for="title">Title</label>
    <input type="text" class="form-control" id="title" name="title" placeholder="Title Here..." autocomplete="off">
    <label for="post">Post</label>
    <textarea name="content" class="form-control" id="post" cols="30" rows="10" placeholder="Begin your post here..." autocomplete="off"></textarea>
    <button class="btn btn-md publish-btn" type="submit" name="publish">Publish</button>
</form>


<%- include("partials/footer") -%> 
